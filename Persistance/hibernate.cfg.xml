<?xml version="1.0" encoding="utf-8"?>
<hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
  <session-factory>
    <property name="connection.provider">CodingArchitect.Spikes.NH.Persistance.SqlLocalDbConnectionProvider, CodingArchitect.Spikes.NH</property>
    <property name="dialect">NHibernate.Dialect.MsSql2012Dialect</property>
    <property name="connection.driver_class">CodingArchitect.Spikes.NH.Persistance.SqlLocalDbClientDriver, CodingArchitect.Spikes.NH</property>
    <property name="connection.connection_string"></property>
    <property name="connection.release_mode">auto</property>
    <property name="show_sql">true</property>
  </session-factory>
</hibernate-configuration>